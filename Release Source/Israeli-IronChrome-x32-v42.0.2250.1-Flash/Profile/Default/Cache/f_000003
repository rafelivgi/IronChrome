<script language="JavaScript"><!--

function change_link_to_post () {
    var all_links = document.getElementsByTagName("a");
    for (i = 0; i < all_links.length; i++) {

        if (all_links[i].className.match(/blockquote/) ||
            all_links[i].className.match(/moreres2/)||all_links[i].className.match(/proxy_link/)||
							            all_links[i].className.match(/no-date/)||all_links[i].className.match(/related_search/) ||
							            all_links[i].className.match(/tod/) || all_links[i].className.match(/video_family_filter/) || 
							            all_links[i].className.match(/video_sort_order/) || all_links[i].className.match(/sugg/)) {

          all_links[i].onclick = function () {
              url_parts = this.href.split("?");
              parameters = url_parts[1];
              key_value_pairs = parameters.split("&");
              if (key_value_pairs.length > 0) {
                  myform = document.createElement("form");
                  myform.setAttribute("method", "POST");
                  myform.setAttribute("action", url_parts[0]);
                  if (this.className.match(/proxy_link/)){
                  myform.setAttribute("target", "_blank");}
                  for (j = 0; j < key_value_pairs.length; j++) {

                      var key_and_value = key_value_pairs[j].split("=");
                      form_elem = document.createElement("input");
                      form_elem.setAttribute("name", key_and_value[0]);
                      form_elem.setAttribute("type", "hidden");
                      key_and_value[1] = decodeURIComponent(key_and_value[1].replace(/\+/g,  ' '));
                      form_elem.setAttribute("value", key_and_value[1]);
                      myform.appendChild(form_elem);
                  }
                  document.getElementById("hide_form").appendChild(myform);
                  myform.submit();
                  return false;
              } else {
                  return true;
              }
          }
       }
    }
}
change_link_to_post();
function findPos(obj) {
					var curleft = curtop = 0;
					if (obj.offsetParent) {
                        curleft = obj.offsetLeft;
                        curtop = obj.offsetTop;
						while (obj = obj.offsetParent) {
                            curleft += obj.offsetLeft;
                            curtop += obj.offsetTop;
						}
					}
					return [curleft,curtop];
				}
				function showMapDetail() {
                    clearTimeout(MapDetailTimeoutId);
                    var offset = findPos(getelem('map_container'));
                    var mapdl = getelem('map-detail');
                    mapdl.style.left = (offset[0] - 10) + 'px';
                    mapdl.style.top = (offset[1] + 259) + 'px';
                    mapdl.style.display = 'block';
					isMapDetailActive = true;

                    jQuery('#mapd').click( function(){
                       isMapDetailActive = true;
                       jQuery('#mapd').removeAttr("onmouseout").removeAttr("onmouseover");
                       jQuery('#map-detail').removeAttr("onmouseout").removeAttr("onmouseover");
                    });
                    jQuery('#map-detail a.popup_close').click( function(){
                       isMapDetailActive = false;
                       jQuery('#map-detail').hide();
                       getelem('mapd').onmouseover = function(){showMapDetail()};
                       getelem('mapd').onmouseout = function(){hideMapDetail()};
                       getelem('map-detail').onmouseover = function(){isMapDetailActive = true; clearTimeout(MapDetailTimeoutId);};
                       getelem('map-detail').onmouseout = function(){hideMapDetail()};
                    });

				}

			var originalPositionOfPrivacyDiv = 0; 
				function showPrivacy() {
					    if(originalPositionOfPrivacyDiv == 0) {
					    originalPositionOfPrivacyDiv = parseInt($('#privacy').css('top'),10);
					    }
					clearTimeout(privacyTimeoutId);
                    var offset = findPos(getelem('query_top'));
					//var offset2 = (document.getElementById('query_top').offsetWidth+35-626)/2;
                    var offset2 = (document.getElementById('query_top').offsetWidth+35-650)/2;
                    var privacy = getelem('privacy');
                    privacy.style.left = (offset[0] + offset2) + 'px';
                    var resultsPresent = 1;
                    var targetElement = getelem('first-result');
                    var sponsoredElement = getelem('sponsored');
		    
		    if (sponsoredElement == null && $('#maths_results').css('display') != 'none') {
			    var mathDivHeight = parseInt($('#maths_results').css('height'),10);
			    privacy.style.top = (findPos(maths_results)[1]+mathDivHeight +27) +'px';
		    } else if (sponsoredElement == null && $('#flight_status').css('display') != 'none') {
                var flightRegDiv = parseInt($('#flight_registration').css('height'),10);
                var flightDivHeight = parseInt($('#flight_status').css('height'),10);
                privacy.style.top = (findPos(privacy)[1]+ flightDivHeight + flightRegDiv + 60) +'px';
            }else {
                    if (targetElement == null) {
                       targetElement = getelem('no_results');
                       resultsPresent = 0;
                    }
                    if( targetElement != null && sponsoredElement != null) {
                        var topMargin = 0;
                        if (resultsPresent == 1) {
                            topMargin = 20;
                        }
                        privacy.style.top = (findPos(targetElement)[1] + topMargin) + 'px';
		    } else {
			privacy.style.top = originalPositionOfPrivacyDiv + 'px';
		    }
		    }
                    privacy.style.display = 'block';
					isActive = true;
					getelem('query_top').blur();
                    jQuery('#detail').click( function(){
        isActive = true;
        jQuery('#detail').removeAttr("onmouseout").removeAttr("onmouseover");
        jQuery('#privacy').removeAttr("onmouseout").removeAttr("onmouseover");
        });
        jQuery('#privacy a.popup_close').click( function(){
        isActive = false;
        jQuery('#privacy').hide();
        getelem('detail').onmouseover = function(){showPrivacy()};
        getelem('detail').onmouseout = function(){hidePrivacy()};
        getelem('privacy').onmouseover = function(){isActive = true; clearTimeout(privacyTimeoutId);};
        getelem('privacy').onmouseout = function(){hidePrivacy2()};
        });

				}
                function showPopup() {
					clearTimeout(privacyTimeoutId);
                    var offset = findPos(getelem('query_top'));
                    var offset2 = (getelem('query_top').offsetWidth+35-650)/2;
                    var privacy = getelem('popupbox');
                    privacy.style.left = (offset[0] + offset2) + 'px';
                    var targetElement = getelem('first-result');
                    var sponsoredElement = getelem('sponsored');
                    if( targetElement != null && sponsoredElement != null) {
                        privacy.style.top = (findPos(targetElement)[1] + 20) + 'px';
                    }
                    privacy.style.display = 'block';
					isActive = true;
					getelem('query_top').blur();
				}
				var isActive = false;
                var isMapDetailActive = false;
				var privacyTimeoutId, MapDetailTimeoutId;
				function checkAndHidePrivacy() {
					if(!isActive) {
						getelem('query_top').focus();
                        getelem('privacy').style.display = 'none';
                        isActive = false;
					}
				}
				function checkAndHideMapDetail() {
					if(!isMapDetailActive) {
                        getelem('map-detail').style.display = 'none';
                        isMapDetailActive = false;
					}
				}

				function hidePrivacy() { isActive = false; privacyTimeoutId = setTimeout('checkAndHidePrivacy()', 400); }
				function hidePrivacy2() { isActive = false; privacyTimeoutId = setTimeout('checkAndHidePrivacy()', 10); }
				function hideMapDetail() { isMapDetailActive = false; MapDetailTimeoutId = setTimeout('checkAndHideMapDetail()', 10); }
