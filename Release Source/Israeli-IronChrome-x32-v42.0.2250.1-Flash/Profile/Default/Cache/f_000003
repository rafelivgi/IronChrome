function mysubmit (myID) {
    if (document.pnform && document.pnform.startat) {
        document.pnform.startat.value = myID;
        document.pnform.submit();
    }
}

function set_tip_width(hover_text, width) {
    var agentname = navigator.userAgent;
    if ((agentname.indexOf('Mac') != -1) && (agentname.toLowerCase().indexOf('firefox') != -1)) {
        Tip(hover_text, WIDTH, width);
    } else {
        Tip(hover_text);
    }
}
</script>
