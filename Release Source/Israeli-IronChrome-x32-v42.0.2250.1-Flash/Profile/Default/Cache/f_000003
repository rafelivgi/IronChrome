e_urls = new Array();
var c = 0;

function eu (fg, target) { // eu->engine url
    var u = e_urls[fg-1];
    if (target !== '') {
        c++;
        window.open(u, 'wn'+c, '');
    } else {
        location.href = u;
    }
    return false;
}
