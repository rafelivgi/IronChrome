{
   "background": {
      "scripts": [ "background.js" ]
   },
   "description": "BarTab inspired (but infinately more primitive) utility for blocking background tabs from killing Chrome on start.",
   "homepage_url": "http://sourceforge.net/projects/footab/",
   "icons": {
      "128": "footab128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "FooTab",
   "permissions": [ "webRequest", "webRequestBlocking", "storage", "tabs", "*://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "13.3.5"
}
